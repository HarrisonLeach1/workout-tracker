{
  "expo": {
    "name": "Workout Tracker",
    "slug": "workout-tracker",
    "scheme": "workouttracker",
    "privacy": "unlisted",
    "description": "An app to create workout routines and exercises to track your workout progress during self-isolation.",
    "platforms": ["ios", "android", "web"],
    "androidStatusBar": {
      "hidden": true
    },
    "entryPoint": "./src/App.tsx",
    "version": "0.2.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    }
  }
}
